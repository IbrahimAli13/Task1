<p style="color: green"><%= notice %></p>

<h1>Carinfos</h1>

<table class="table table-dark table-striped">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Car Id</th>
      <th scope="col">Plate</th>
      <th scope="col">Owner</th>
      <th scope="col">action</th>
    </tr>
  </thead>
  <tbody>
  <%@carinfos.each do |carinfos|%>
    <tr>
    <th scope="row"><%=carinfos.id%></th>
    <td><%=carinfos.car_id%></td>
    <td><%=carinfos.plate%></td>
    <td><%=carinfos.owner%></td>
    <td><%= link_to "show", carinfo_path(carinfos.id),class:"btn btn-light" %></td>
  </tr>
    <%end%>
   
  </tbody>
</table>

<%= link_to "New carinfo", new_carinfo_path %>
